import random
number = random.randint(1,100)
guess = 0

print(f"This is guessing game")
print("My guess is between 1 and 100")
print("Can you find it?")
while guess != number:
 
 guess = int(input("Enter guess:"))

if (guess < number):
 print("Guess higher!")
elif (guess > number):
 print ("Guess lower!")
else:
 print("Your guess is correct!")