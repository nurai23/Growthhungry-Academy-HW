
class bank_account:
  name = None
  balance = None

bank_account1 = bank_account()
bank_account1.name = Nurai
bank_account1.balance = 3000


bank_account2 = bank_account()
bank_account2.name = Ailin
bank_account2.balance = 5000

bank_account3 = bank_account()
bank_account3.name = Aidar
bank_account3.balance = 4000

print(bank_account1)