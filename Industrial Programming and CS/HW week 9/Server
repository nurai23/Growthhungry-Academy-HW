# server

import socket

# 1 Server's set up

HOST = "127.0.0.1"

PORT = 65432

with socket.socket(socket.AF_INET, socket.SOCK_STREAM) as s:
    s.bind((HOST, PORT))
    s.listen()
    conn, addr = s.accept()

# 3 Data is exchanged
with conn:
    print(f"Connected by {addr}")
    while True:
     data = conn.recv(1024)
     if not data:
      break
    conn.sendall(data)