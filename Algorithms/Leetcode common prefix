from typing import List


class Solution:
  def longestCommonPrefix(strs: List[str]) -> str:
    prefix = ""
    for chars in zip(*strs):
        if len(set(chars)) == 1:
            prefix += chars[0]
        else:
            break
    return prefix  # Moved outside the loop

  a = ["navy", "navigation", "navigator"]
  print(longestCommonPrefix(a))